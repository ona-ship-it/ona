    "start": "next start -p 3000",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "cross-env TZ=UTC jest --runInBand",
    "test:unit": "node tests/unit-tests.js",
    "test:testnet": "node tests/testnet-comprehensive.js",
