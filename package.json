    "dev:full": "concurrently \"npm run dev\" \"npm run wallet:start\"",
    "admin:test-giveaway": "node scripts/run-admin-giveaway-tests.mjs",
    "admin:test-giveaway:publish": "node scripts/run-admin-giveaway-tests.mjs --action publish",
    "admin:test-giveaway:unpublish": "node scripts/run-admin-giveaway-tests.mjs --action unpublish"
  },
